%div{ :data => {:role => "content" }}
  %ul{:id=>"set_list",  :data => {:filter=>"true", :role=> "listview" }}
    %li(data-role="list-divider")Filters
    - @sets.each do |set|
      %li
        %a{:href=>edit_training_set_path(set)}=set.name
        %a{:href=>"#", 'data-icon'=>"minus"}
    %li(data-icon="plus")
      %a(href=new_training_set_path)Add new set
